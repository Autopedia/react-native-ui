{
  "name": "@autopedia/react-native-ui",
  "description": "A React Native UI components for React Native apps of doctor-cha services.",
  "version": "2.0.2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Autopedia/react-native-ui"
  },
  "bugs": {
    "url": "https://github.com/Autopedia/react-native-ui/issues"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "postbuild": "cp -R src/assets lib/assets",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "color": "^3.1.3",
    "lodash": "^4.17.21",
    "react-hook-form": "^7.8.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-hyperlink": "^0.0.19",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.28.0",
    "react-native-parsed-text": "^0.0.22",
    "react-native-rate": "^1.2.6",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-video": "^5.1.1",
    "styled-components": "^5.3.0",
    "validate-color": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/react-native": "^7.2.0",
    "@types/color": "^3.0.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/react-native": "^0.64.0",
    "@types/react-native-actionsheet": "^2.4.2",
    "@types/react-native-maps": "^0.24.2",
    "@types/react-native-video": "^5.0.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.9",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.14.0",
    "graceful-fs": "^4.2.6",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.4",
    "metro-react-native-babel-preset": "^0.64.0",
    "react": "17.0.1",
    "react-dom": "^17.0.2",
    "react-native": "0.64.0",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.1.4",
    "typescript-transform-paths": "^3.0.1"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "**/*.tsx",
      "!**/*.decorators.tsx",
      "!**/*.stories.tsx"
    ],
    "coverageDirectory": "./coverage",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transformIgnorePatterns": [
      "nodemodules/(?!(react-native-swipe-gestures)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.js"
    ],
    "testEnvironmentOptions": {
      "resources": "usable"
    }
  }
}
